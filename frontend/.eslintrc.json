{
	"extends": [
		"next/core-web-vitals",
		"next/typescript",
		"prettier",
		"plugin:@next/next/recommended",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": ["error", { "endOfLine": "auto" }],
		"react/react-in-jsx-scope": "off"
	},
	"plugins": ["prettier"]
}
